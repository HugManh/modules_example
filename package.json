{
  "name": "gallery-dino",
  "version": "1.0.0",
  "description": "gallery-dino",
  "main": "index.js",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "build:server": "babel src/server -d build/server --copy-files --minified --no-comments",
    "format": "prettier --write \"src/**/*.{js,ts}\" \"test/**/*.{js,ts}\"",
    "start:prod": "babel-node src/server",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/server",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{js,ts}\" --fix",
    "build": "npm run clean && npm run build:server",
    "clean": "rm -rf build && mkdir build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cloudinary": "^2.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "datauri": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "streamifier": "^0.1.1",
    "vite": "^5.2.9"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "cross-env": "^7.0.3",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "nodemon": "^3.0.3"
  }
}
